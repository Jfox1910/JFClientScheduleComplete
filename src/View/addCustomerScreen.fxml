<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="480.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/11.0.14-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.AddCustomerController">
   <children>
      <VBox alignment="CENTER" layoutY="170.0" prefHeight="49.0" prefWidth="640.0">
         <children>
            <TextField fx:id="CustomerAddress" alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefWidth="530.0" promptText="ADDRESS">
               <VBox.margin>
                  <Insets bottom="10.0" top="10.0" />
               </VBox.margin>
            </TextField>
         </children>
      </VBox>
      <HBox alignment="CENTER" layoutY="380.0" prefHeight="100.0" prefWidth="640.0">
         <children>
            <Button fx:id="addCustomer" mnemonicParsing="false" onAction="#onActionSaveCustomer" text="Add Customer">
               <padding>
                  <Insets bottom="5.0" left="10.0" right="10.0" top="5.0" />
               </padding>
               <HBox.margin>
                  <Insets left="10.0" right="10.0" top="10.0" />
               </HBox.margin>
            </Button>
            <Button fx:id="cancelButton" mnemonicParsing="false" onAction="#onActionMainScreen" text="Cancel">
               <HBox.margin>
                  <Insets left="10.0" right="10.0" top="10.0" />
               </HBox.margin>
               <padding>
                  <Insets bottom="5.0" left="10.0" right="10.0" top="5.0" />
               </padding>
            </Button>
         </children>
      </HBox>
      <HBox alignment="CENTER" layoutY="-2.0" prefHeight="56.0" prefWidth="640.0">
         <children>
            <Label text="ADD CUSTOMER" textFill="#2600ff">
               <font>
                  <Font name="System Bold" size="18.0" />
               </font>
            </Label>
         </children>
      </HBox>
      <HBox alignment="CENTER" layoutY="207.0" prefHeight="70.0" prefWidth="640.0">
         <children>
            <ComboBox fx:id="customerCountry" onAction="#handleCountryComboBox" prefWidth="100.0" promptText="Country">
               <HBox.margin>
                  <Insets right="20.0" />
               </HBox.margin>
            </ComboBox>
            <ComboBox fx:id="customerDivision" onAction="#handleDivisionComboBox" prefWidth="100.0" promptText="Select:">
               <HBox.margin>
                  <Insets left="20.0" />
               </HBox.margin>
            </ComboBox>
            <TextField fx:id="CustomerZip" alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefWidth="75.0" promptText="ZIP CODE">
               <HBox.margin>
                  <Insets left="20.0" right="20.0" />
               </HBox.margin>
            </TextField>
            <TextField fx:id="CustomerPhone" alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefWidth="175.0" promptText="PHONE NUMBER" />
         </children>
      </HBox>
      <HBox alignment="CENTER" layoutX="-2.0" layoutY="114.0" prefHeight="56.0" prefWidth="640.0">
         <children>
            <TextField fx:id="CustomerName" alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefWidth="400.0" promptText="CUSTOMER NAME">
               <HBox.margin>
                  <Insets left="10.0" />
               </HBox.margin>
            </TextField>
         </children>
      </HBox>
      <VBox alignment="CENTER" layoutX="1.0" layoutY="45.0" prefHeight="56.0" prefWidth="640.0">
         <children>
            <Label />
         </children>
      </VBox>
   </children>
</AnchorPane>
